import java.io.*;
import java.util.Scanner;

public class TempFileDiff {

	public static void main(String[] args) throws FileNotFoundException {
		Scanner console = new Scanner(new File("src/weather.txt"));		
		double change = 0.0;
		double first, second, next;
		while (console.hasNextDouble()){
			first = console.nextDouble(); 
			second = console.nextDouble();
			change = second - first;
			System.out.println(first + " to " + second + ", change = " + change);
			next = second; 
			double nextN = console.nextDouble();
			change = second - nextN;
			System.out.println(next + " to " + nextN + ", change = " + change);
		}
	}
}
