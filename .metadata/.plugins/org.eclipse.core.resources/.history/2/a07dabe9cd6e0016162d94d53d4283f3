/**
 * @author Donna
 *
 */
public class Calculate {
	
	public static double square(double number){
		double answer = number *number; 
		return answer; 
	}
	
	public static double average(double number){
		double answer = number +number;
		answer = answer * 1/2;
		return answer;
	}
	
	public static double cube(double number){
		double answer = number *number * number; 
		return answer; 
	}
	public static double average3(double number){
		double answer = number +number+number;
		answer = answer * 1/3;
		return answer;
	}

}
