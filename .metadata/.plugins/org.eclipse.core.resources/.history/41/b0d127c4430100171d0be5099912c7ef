package textExcel;

public class SpreadsheetLocation implements Location{
	int row;
	String column;
	
    @Override
    public int getRow(){
        row -= 1;
        return row;
    }

    @Override
    public int getCol(){
        if (column.equals('A')){
        	return 0;
        } else if(column.equals('L')){
        	return 11;
        } else {
        	return 3;
        }
    }
    
    public SpreadsheetLocation(String cellName){
        row = Integer.parseInt(cellName.substring(1));
        column = cellName.substring(0);;
    	
    }

}
